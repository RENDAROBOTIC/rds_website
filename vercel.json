{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)\\.(html)$",
      "dest": "/$1.html"
    },
    {
      "src": "/",
      "dest": "renda_design_supply.html"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
