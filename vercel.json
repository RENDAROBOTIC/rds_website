{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "!(server.js)",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/style_sheet/(.*)",
      "dest": "/style_sheet/$1"
    },
    {
      "src": "/checkout/(.*)",
      "dest": "/checkout/$1"
    },
    {
      "src": "/tools/(.*)",
      "dest": "/tools/$1"
    },
    {
      "src": "/n&t/(.*)",
      "dest": "/n&t/$1"
    },
    {
      "src": "/products/(.*)",
      "dest": "/products/$1"
    },
    {
      "src": "/search/(.*)",
      "dest": "/search/$1"
    },
    {
      "src": "/support/(.*)",
      "dest": "/support/$1"
    },
    {
      "src": "/event/(.*)",
      "dest": "/event/$1"
    },
    {
      "src": "/",
      "dest": "/renda_design_supply.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
